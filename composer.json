{
  "name": "andersundsehr/metrics_exporter",
  "description": "Prometheus metrics collector for TYPO3. Collect and export metrics.",
  "license": "GPL-2.0-or-later",
  "type": "typo3-cms-extension",
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
    "ext-pdo": "*",
    "promphp/prometheus_client_php": "^2.14.1",
    "typo3/cms-core": "^11.5 || ^12.4 || ^13.4"
  },
  "require-dev": {
    "pluswerk/grumphp-config": "*",
    "saschaegerer/phpstan-typo3": "*",
    "ssch/typo3-rector": "^2.5.0",
    "typo3/testing-framework": "*"
  },
  "autoload": {
    "psr-4": {
      "AUS\\MetricsExporter\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AUS\\MetricsExporter\\Tests\\": "Tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "phpro/grumphp": true,
      "phpstan/extension-installer": true,
      "pluswerk/grumphp-config": true,
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    },
    "lock": false
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "metrics_exporter"
    }
  },
  "scripts": {
    "test": "@php ./vendor/bin/phpunit --color=always --testdox --stderr --display-deprecations --display-notices --display-warnings --bootstrap FunctionalTestsBootstrap.php Tests/Functional/"
  }
}
